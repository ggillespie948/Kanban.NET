@model IEnumerable<IssueTracker.Models.ProjectModels>

@{
    ViewBag.Title = "Index";
}



<h2>My Projects</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OwnerUser.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationUserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td class="column1">
            @Html.DisplayFor(modelItem => item.OwnerUser.Email)
        </td>
        <td class="column2">
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td class="column3">
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td class="column4">
            @Html.DisplayFor(modelItem => item.ApplicationUserId)
        </td>
        <td class="column5">
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
